<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <filter>
        <filter-name>FillterDispatcher</filter-name>
        <filter-class>phuclh.fillter.FillterDispatcher</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>FillterDispatcher</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>
    <servlet>
        <servlet-name>AcctiveAccountServlet</servlet-name>
        <servlet-class>phuclh.controllers.AcctiveAccountServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CreateNewAccountServlet</servlet-name>
        <servlet-class>phuclh.controllers.CreateNewAccountServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetCodeActiveServlet</servlet-name>
        <servlet-class>phuclh.controllers.GetCodeActiveServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>phuclh.controllers.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogoutServlet</servlet-name>
        <servlet-class>phuclh.controllers.LogoutServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoadServlet</servlet-name>
        <servlet-class>phuclh.controllers.LoadServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PostArticleServlet</servlet-name>
        <servlet-class>phuclh.controllers.PostArticleServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SearchServlet</servlet-name>
        <servlet-class>phuclh.controllers.SearchServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DetailServlet</servlet-name>
        <servlet-class>phuclh.controllers.DetailServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PostCommentServlet</servlet-name>
        <servlet-class>phuclh.controllers.PostCommentServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoadCommentServlet</servlet-name>
        <servlet-class>phuclh.controllers.LoadCommentServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteArticleServlet</servlet-name>
        <servlet-class>phuclh.controllers.DeleteArticleServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ActiveArticleServlet</servlet-name>
        <servlet-class>phuclh.controllers.ActiveArticleServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AcctiveAccountServlet</servlet-name>
        <url-pattern>/AcctiveAccountServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CreateNewAccountServlet</servlet-name>
        <url-pattern>/CreateNewAccountServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetCodeActiveServlet</servlet-name>
        <url-pattern>/GetCodeActiveServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogoutServlet</servlet-name>
        <url-pattern>/LogoutServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoadServlet</servlet-name>
        <url-pattern>/LoadServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PostArticleServlet</servlet-name>
        <url-pattern>/PostArticleServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SearchServlet</servlet-name>
        <url-pattern>/SearchServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DetailServlet</servlet-name>
        <url-pattern>/DetailServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PostCommentServlet</servlet-name>
        <url-pattern>/PostCommentServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoadCommentServlet</servlet-name>
        <url-pattern>/LoadCommentServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteArticleServlet</servlet-name>
        <url-pattern>/DeleteArticleServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ActiveArticleServlet</servlet-name>
        <url-pattern>/ActiveArticleServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
